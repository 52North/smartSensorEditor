			<!--The weight of the hydrophone -->
			<sml:characteristic name="weight">
				<swe:Quantity
					definition="http://www.nexosproject.eu/dictionary/definitions.html#weight">
					<swe:label>Weight (kg)</swe:label>
					<swe:uom code="kg" />
					<swe:value>0.423</swe:value>
				</swe:Quantity>
			</sml:characteristic>
			<!--The length of the hydrophone -->
			<sml:characteristic name="length">
				<swe:Quantity
					definition="http://www.nexosproject.eu/dictionary/definitions.html#length">
					<swe:label>Length (mm)</swe:label>
					<swe:uom code="mm" />
					<swe:value>35</swe:value>
				</swe:Quantity>
			</sml:characteristic>
			<!--The width of the hydrophone -->
			<sml:characteristic name="width">
				<swe:Quantity
					definition="http://www.nexosproject.eu/dictionary/definitions.html#width">
