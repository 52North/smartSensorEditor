			<sml:characteristic name="batteryCurrent">
				<swe:Quantity
					definition="http://www.nexosproject.eu/dictionary/definitions.html#batteryCurrent">
					<swe:identifier>testIdentifierQuantity</swe:identifier>
					<swe:label>Battery Current (mA)</swe:label>
